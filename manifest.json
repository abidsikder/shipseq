{
  "name": "ShipSeq",
  "version": "2022.6.25",
  "description": "Net-zero your package deliveres.",
  "manifest_version": 3,
  "author": "Md Abid Sikder",
  "icons" : {
    "128" : "logo.png"
  },
  "action":{
    "default_popup" : "popup.html",
    "default_title": "ShipSeq"
  },
  "permissions" : [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions" : [
    "https://*/*"
  ],
  "background": {
    "service_worker" : "background.js"
  }
}
